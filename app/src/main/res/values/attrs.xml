<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="radius" format="dimension" />

    <declare-styleable name="CircularImageView">
        <attr name="radius" />
        <attr name="circle_color" format="color" />
        <attr name="src" format="reference" />
